<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:paddingLeft="8dp"
		android:paddingTop="30dp"
		android:paddingRight="8dp"
		android:paddingBottom="30dp"
		android:gravity="center_horizontal"
		android:orientation="vertical"
		android:background="@drawable/dialog">
		<TextView
			android:id="@+id/textview9"
			android:layout_width="200dp"
			android:layout_height="wrap_content"
			android:layout_marginBottom="30dp"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="Kollektionen"
			android:textSize="30sp"
			android:textStyle="bold"
			android:textColor="@color/text" />
		<TextView
			android:id="@+id/textview12"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:text="Etwas leer hier ðŸ—¿"
			android:textSize="20sp"
			android:textColor="@color/text"
			android:alpha="0.7"
			android:layout_weight="1" />
		<LinearLayout
			android:id="@+id/linear7"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear8"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview10"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Kollektion: "
					android:textSize="16sp"
					android:textColor="@color/text" />
				<TextView
					android:id="@+id/textview11"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="null"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="@color/text" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear10"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview2"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:src="@drawable/ic_file_download"
					android:scaleType="fitCenter" />
				<TextView
					android:id="@+id/textview6"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right"
					android:text="Importieren"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="@color/text" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear11"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview3"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:src="@drawable/ic_file_upload"
					android:scaleType="fitCenter" />
				<TextView
					android:id="@+id/textview7"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right"
					android:text="Exportieren"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="@color/text" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear13"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview6"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:src="@drawable/ic16"
					android:scaleType="fitCenter" />
				<TextView
					android:id="@+id/textview14"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right"
					android:text="Datenanalyse exportieren"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="@color/text" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview4"
					android:layout_width="30dp"
					android:layout_height="30dp"
					android:src="@drawable/ic_delete"
					android:scaleType="fitCenter" />
				<TextView
					android:id="@+id/textview8"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="right"
					android:text="LÃ¶schen"
					android:textSize="16sp"
					android:textStyle="bold"
					android:textColor="@color/text" />
			</LinearLayout>
		</LinearLayout>
		<ListView
			android:id="@+id/listview1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:dividerHeight="0dp"
			android:divider="@null"
			android:choiceMode="none"
			android:layout_weight="1" />
		<Button
			android:id="@+id/button9"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_margin="8dp"
			android:padding="12dp"
			android:text="Neuer Slot"
			android:textSize="16sp"
			android:textColor="@color/text" />
	</LinearLayout>
</LinearLayout>
